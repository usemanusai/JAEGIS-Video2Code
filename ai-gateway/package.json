{
  "name": "video2code-ai-gateway",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": { "node": ">=22" },
  "scripts": {
    "dev": "node --watch src/index.js",
    "dev:safe": "pwsh ../scripts/dev-preflight.ps1; if ($LASTEXITCODE -eq 0 -or $LASTEXITCODE -eq 2) { node --watch src/index.js }",
    "start": "node src/index.js",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "node tests/run-tests.mjs",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov"
  },
  "dependencies": {
    "express": "5.1.0",
    "multer": "^2.0.0"
,
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "eslint": "9.34.0",
    "@eslint/js": "9.34.0",
    "prettier": "3.6.2",
    "jest": "30.1.3",
    "@types/jest": "29.5.12",
    "@types/express": "5.0.0"
  }
}

