{
  "name": "video2code-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": { "node": ">=22" },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "lint": "eslint . --config eslint.config.js",
    "format": "prettier -w .",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-syntax-highlighter": "15.6.1",
    "react-router-dom": "6.26.2"
  },
  "devDependencies": {
    "vite": "7.1.4",
    "@vitejs/plugin-react": "5.0.0",
    "typescript": "5.9.2",
    "eslint": "9.34.0",
    "@eslint/js": "9.34.0",
    "prettier": "3.6.2",
    "jest": "30.1.3",
    "@types/jest": "29.5.12",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}

